{
  "name": "hutao-gs",
  "version": "1.0.0",
  "description": "HuTao GS",
  "main": "src/entry/mainEntry.ts",
  "bin": "buildDev/entry/mainEntry.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "tsc --noEmit",
    "build-dev": "node ./build -mode:dev",
    "build": "node ./build",
    "pack-exe": "pkg . -o \"dist/HuTao-GS.exe\" --build",
    "start": "node ./buildDev/entry/mainEntry.js",
    "start:dev": "node ./build -mode:dev && node ./buildDev/entry/mainEntry.js",
    "prepare": "husky install",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "format": "prettier -w \"./src/**/*.ts\""
  },
  "pkg": {
    "assets": [
      "webstaticSea/**/*"
    ],
    "scripts": [
      "buildDev/**/*.js"
    ],
    "targets": [
      "node18-win"
    ]
  },
  "author": "NotArandomGUY",
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "denque": "^2.1.0",
    "fs": "^0.0.1-security",
    "hostile": "^1.3.3",
    "md5": "^2.3.0",
    "promise-events": "^0.2.4",
    "protobufjs": "^7.2.3",
    "source-map-support": "^0.5.21",
    "verror": "^1.10.1",
    "wasmoon": "https://github.com/kuma-dayo/wasmoon.git",
    "yieldable-json": "^2.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/hostile": "^1.3.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.11",
    "@types/source-map-support": "^0.5.6",
    "@types/verror": "^1.10.6",
    "@types/yieldable-json": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "pkg": "^5.8.1",
    "prettier": "^2.8.7",
    "resedit": "^1.7.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.15.0"
  }
}
